extends layout-scan

block content
  include header.jade
  .container
    input.qrcode-text(type='text', size='16', placeholder='Tracking Code')
    label.qrcode-text-btn
      input(type='file', accept='image/*', capture='environment', onclick='return showQRIntro();', onchange='openQRCamera(this);', tabindex='-1')
    input(type='button', value='Go', disabled)
  include footer.jade
